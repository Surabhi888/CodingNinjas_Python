    "n=int(input())\n",
    "sum=0\n",
    "order=len(str(n))\n",
    "temp=n\n",
    "while(n>0):\n",
    "    digit=n%10\n",
    "    sum+=digit**order\n",
    "    n=n//10\n",
    "if(sum==temp):\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
