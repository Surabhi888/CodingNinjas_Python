    "def checkPalindrome(num):\n",
    "    temp=num\n",
    "    rev=0\n",
    "    while(num>0):\n",
    "        dig=num%10\n",
    "        rev=rev*10+dig\n",
    "        num=num//10\n",
    "    return rev\n",
    "pass\n",
    "\t\t\n",
    "num = int(input())\n",
    "temp=num\n",
    "isPalindrome = checkPalindrome(num)\n",
    "if(temp==isPalindrome):\n",
    "\tprint(\"true\")\n",
    "else:\n",
    "\tprint(\"false\")"
   ]
