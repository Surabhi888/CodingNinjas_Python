   "source": [
    "n = int(input())\n",
    "s = 0\n",
    "t = n\n",
    "a=len(str(n))\n",
    "while t > 0:\n",
    "\n",
    "   digit = t % 10\n",
    "\n",
    "   s += digit ** a\n",
    "   t //= 10\n",
    "if n == s:\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
  }
